    "setup:assets": "node scripts/setup-transformers-assets.js",
    "package": "plasmo package",
    "share": "npm run build:prod && node scripts/create-share-package.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "postinstall": "patch-package && node scripts/fix-onnx-imports.js"
  },
