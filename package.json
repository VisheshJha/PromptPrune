  "displayName": "PromptPrune",
  "version": "1.0.0-beta.1",
  "description": "Optimize AI prompts to reduce token usage and costs using local processing",
  "author": "PromptPrune Team",
  "scripts": {
    "dev": "node scripts/fix-dev-manifest.js; (sleep 10 && node scripts/fix-dev-manifest.js) & plasmo dev",
    "build": "npm run setup:assets && plasmo build && node scripts/fix-manifest.js",
    "build:dev": "GROOT_API_URL=http://localhost:8080/api/v1 npm run build",
