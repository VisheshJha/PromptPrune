{
  "name": "promptprune",
  "displayName": "PromptPrune",
  "version": "1.0.0",
  "description": "Optimize AI prompts to reduce token usage and costs using local processing",
  "author": "PromptPrune Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build && node scripts/fix-manifest.js",
    "package": "plasmo package",
    "share": "npm run build && node scripts/create-share-package.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "compromise": "^14.14.4",
    "js-tiktoken": "^1.0.21",
    "plasmo": "^0.88.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/chrome": "^0.0.251",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "glob": "^11.0.3",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "manifest": {
    "host_permissions": [
      "http://localhost/*",
      "http://127.0.0.1/*",
      "https://chat.openai.com/*",
      "https://chatgpt.com/*",
      "https://claude.ai/*",
      "https://gemini.google.com/*",
      "https://www.perplexity.ai/*",
      "https://perplexity.ai/*",
      "https://poe.com/*",
      "https://grok.com/*",
      "https://x.com/*",
      "https://twitter.com/*",
      "https://copilot.microsoft.com/*",
      "https://manus.im/*",
      "https://www.deepseek.com/*",
      "https://deepseek.com/*",
      "https://www.midjourney.com/*",
      "https://midjourney.com/*"
    ],
    "permissions": [
      "storage",
      "unlimitedStorage",
      "scripting",
      "tabs"
    ]
  }
}
